{
  "name": "vue-tinder",
  "version": "2.0.2",
  "description": "Imitate Tinder by Vue.js",
  "author": {
    "name": "JohnnyDan",
    "email": "shanlh841271734@live.com"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rollup -c rollup.config.js",
    "lint": "vue-cli-service lint",
    "docs:build": "vuepress build docs",
    "docs:deploy": "bash deploy_docs.sh",
    "docs:dev": "vuepress dev docs",
    "release": "bash release.sh"
  },
  "main": "lib/vue-tinder.js",
  "dependencies": {
    "core-js": "^3.8.1",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@vue/cli-plugin-babel": "~5.0.0-alpha.3",
    "@vue/cli-plugin-eslint": "~5.0.0-alpha.3",
    "@vue/cli-service": "~5.0.0-alpha.3",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vuepress/plugin-google-analytics": "^1.2.0",
    "eslint": "^7.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-vue": "^7.2.0",
    "prettier": "^2.2.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.3",
    "rollup-plugin-vue": "^5.0.1",
    "vuepress": "^1.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "@vue/prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-console": 1,
      "vue/no-v-model-argument": 0
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "homepage": "https://github.com/shanlh/vue-tinder",
  "keywords": [
    "tinder",
    "tantan",
    "vue"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shanlh/vue-tinder.git"
  }
}
